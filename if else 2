import kotlin.jvm.JvmStatic as JvmStatic

class MyClass {

    companion object{
         @JvmStatic
         fun main(args: Array<String>) {
         var num= 11
             var msg =""
          msg= if (num>100) "NO. IS GREATER" else "NO. IS SMALLER"
             println(msg)
         }
         }
    }
