import kotlin.jvm.JvmStatic as JvmStatic

class MyClass {

    companion object{
         @JvmStatic
         fun main(args: Array<String>)
         {
             println("THE SUM IS ${add(a:2,b:4,c:7)}")
         }
         fun add(a:Int , b:Int) :Int {
             return a+b

         }
        fun add(a:Int , b:Int,c:Int) :Any {
            return a+b+c

        }
    }

}
